#ifndef SENSOR_H_
#define SENSOR_H_

typedef struct sen {
	short sen;//現在データ
	short old[9];//過去データ
	short ref_wall;//中央時のセンサ値
	short non_threshold;//壁なし
	short diff_threshold;//壁切れ
	short diff;//センサ差分
} SEN;




#endif /* SENSOR_H_ */
